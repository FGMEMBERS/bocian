<?xml version="1.0"?>

<!-- vertical speed indicator -->

<PropertyList>

	<path>vsi.ac</path>
	<texture-path>Textures</texture-path>

	<animation>
		<type>rotate</type>
		<object-name>vsi-Needle1</object-name>
		<property>instrumentation/variometer/te-reading1-mps</property>
		<interpolation>
			<entry><ind>-5</ind><dep>-130</dep></entry>
			<entry><ind>5</ind><dep>130</dep></entry>
		</interpolation>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<!-- *** LoD *** -->
	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>30</max-m>
	</animation>

</PropertyList>
