<?xml version="1.0"?>
<PropertyList> 

	<path>turn.ac</path>
	<texture-path>Textures</texture-path>

	<animation> 
		<type>rotate</type>
		<object-name>turn_needle</object-name>
		<property>orientation/roll-deg</property>
		<interpolation>
			<entry>
				<ind>-50</ind>
				<dep>-50</dep>
			</entry>
			<entry>
				<ind>50</ind>
				<dep>50</dep>
			</entry>
		</interpolation>
		<axis> 
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<center>
			<x-m>0.0</x-m>
			<y-m>0.0</y-m>
			<z-m>-0.0052</z-m>
		</center>
	</animation>
	<animation> 
		<type>rotate</type>
		<object-name>slip_skid_ball</object-name>
		<property>instrumentation/slip-skid-ball/indicated-slip-skid</property>
		<interpolation>
			<entry>
				<ind>-1</ind>
				<dep>-0.6</dep>
			</entry>
			<entry>
				<ind>1</ind>
				<dep>0.6</dep>
			</entry>
		</interpolation>
		<axis> 
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<center>
			<x-m>0.0</x-m>
			<y-m>0.0</y-m>
			<z-m>1.746</z-m>
		</center>
	</animation>

	<!-- *** LoD *** -->
	<animation> 
		<type>range</type>
		<min-m>0</min-m>
		<max-m>10</max-m>
	</animation>
</PropertyList>
