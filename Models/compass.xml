<?xml version="1.0"?>
<PropertyList> 

	<path>compass.ac</path>
	<texture-path>Textures</texture-path>

	<animation> 
		<type>rotate</type>
		<object-name>compass</object-name>
		<property>instrumentation/magnetic-compass/indicated-heading-deg</property>
		<axis> 
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
		<center>
			<x-m>0.0</x-m>
			<y-m>0.0</y-m>
			<z-m>0.0</z-m>
		</center>
	</animation>

	<!-- *** LoD *** -->
	<animation> 
		<type>range</type>
		<min-m>0</min-m>
		<max-m>10</max-m>
	</animation>
</PropertyList>
